{{ blok._editable|raw }}
<div class="c-faq u-pad-t-sm u-pad-b-sm u-pad-t-xs--xs u-pad-b-xs--xs">
    <div class="container">
        <div class="c-faq__inner u-bg-black">
            {% if blok.title %}
                <span class="t-h1 u-block u-marg-b-xs u-orange">{{ blok.title }}</span>
            {% endif %}
            <ul class="c-faq__list u-white">
            {% for item in blok.items %}
                <li>
                    <a href="{{ item.content.link }}" class="t-list-arrow u-bold u-antialiased">{{ item.content.title }}</a>
                </li>
            {% endfor %}
            </ul>
            {% if blok.link and blok.link_title %}
                <a href="{{ blok.link }}" class="btn btn-inverse btn-primary u-antialiased gtm-section-cta-FAQ">{{ blok.link_title }}</a>
            {% endif %}
        </div>
    </div>
</div>
